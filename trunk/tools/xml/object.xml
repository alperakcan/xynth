
<application>
	<window id="demo" type="WINDOW_MAIN">
		<title>Object XML Demo</title>
		<move>
			<x>60</x>
			<y>60</y>
			<w>500</w>
			<h>400</h>
		</move>
		<object id="frame" type="frame">
			<style>
				<shape>FRAME_PANEL</shape>
				<shadow>FRAME_RAISED</shadow>
			</style>
			<move>
				<x>0</x>
				<y>0</y>
				<w>500</w>
				<h>600</h>
			</move>
			<show>yes</show>
			<object id="button_hide" type="button">
				<move>
					<x>5</x>
					<y>5</y>
					<w>70</w>
					<h>20</h>
				</move>
				<show>yes</show>
				<object id="textbox_hide" type="textbox">
					<style>
						<shape>FRAME_NOFRAME</shape>
					</style>
					<string>hide area</string>
					<move>
						<x>0</x>
						<y>0</y>
						<w>70</w>
						<h>20</h>
					</move>
					<show>yes</show>
				</object>
			</object>
			<object id="button_blender" type="button">
				<move>
					<x>80</x>
					<y>5</y>
					<w>55</w>
					<h>20</h>
				</move>
				<show>yes</show>
				<object id="textbox_blender" type="textbox">
					<style>
						<shape>FRAME_NOFRAME</shape>
					</style>
					<string>blender</string>
					<move>
						<x>0</x>
						<y>0</y>
						<w>55</w>
						<h>20</h>
					</move>
					<show>yes</show>
				</object>
			</object>
			<object id="button_focus" type="button">
				<move>
					<x>140</x>
					<y>5</y>
					<w>85</w>
					<h>20</h>
				</move>
				<show>yes</show>
				<object id="textbox_focus" type="textbox">
					<style>
						<shape>FRAME_NOFRAME</shape>
					</style>
					<string>random focus</string>
					<move>
						<x>0</x>
						<y>0</y>
						<w>85</w>
						<h>20</h>
					</move>
					<show>yes</show>
				</object>
			</object>
			<object id="checkbox" type="checkbox">
				<move>
					<x>300</x>
					<y>5</y>
					<w>85</w>
					<h>20</h>
				</move>
				<string>checkbox</string>
				<show>yes</show>
			</object>
		</object>
		<show>yes</show>
	</window>
</application>
